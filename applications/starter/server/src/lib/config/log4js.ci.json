{
  "appenders": [
    {
      "type": "file",
      "___filename_windows": "griso.log",
      "___filename_docker": "/var/log/griso.log",
      "filename": "griso.log",
      "levels": [
        "DEBUG"
      ],
      "layout": {
        "type": "pattern",
        "pattern": "%d{ISO8601} [%-5p] %-20c %m"
      }
    },
    {
      "type": "console",
      "levels": [
        "DEBUG"
      ],
      "layout": {
        "type": "pattern",
        "pattern": "%d{ISO8601} [%[%-5p%]] %[%-20c%] %m"
      }
    }
  ],
  "levels": {
    "[all]": "WARN",
    "ModuleMetadataStorage": "WARN",
    "BootstrapCore": "INFO",
    "BootstrapPlatform": "INFO",
    "BootstrapCommon": "INFO",
    "BootstrapClient": "INFO",
    "BootstrapComponents": "INFO",
    "BootstrapServer": "INFO",
    "MetadataStorage": "WARN",
    "JsonSerializer": "WARN",
    "BaseService": "INFO",
    "PrintService": "DEBUG",
    "PrinterController": "INFO",
    "PassportController": "WARN",
    "PassportLocalService": "WARN",
    "ConfigService": "INFO",
    "ServerBase": "INFO",
    "ExpressServer": "INFO",
    "ExtendedSendResponseMiddleware": "INFO",
    "ArtikelController": "INFO",
    "KollektionController": "INFO",
    "ControllerBase": "INFO"
  }
}