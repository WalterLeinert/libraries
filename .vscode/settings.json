{
  "typescript.tsdk": "./node_modules/typescript/lib",

  "files.autoSave": "afterDelay",
  "window.zoomLevel": 0,
  "workbench.sideBar.location": "left",
  "git.confirmSync": false,
  "[typescript]": {
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "workbench.welcome.enabled": true,
  "window.menuBarVisibility": "default",
  "workbench.statusBar.visible": true,
  "workbench.activityBar.visible": true,
  //"terminal.integrated.shell.windows": "C:\\Program Files\\Git\\git-bash.exe -c \"exec node %*\""

  "tslint.rulesDirectory": "./node_modules/codelyzer",
  "tslint.configFile": "./tslint.json",

  "html.format.extraLiners": "head, body, /html, p-tabView",
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "angulardoc.repoId": "879892b4-5710-40ea-8b21-f4ce9098cc8a",
  "angulardoc.directoryFilter": [
    "!typings",
    "!node_modules",
    "!.vscode"
  ],
  "angulardoc.fileFilter": [
    "**/!(*.*spec).ts"
  ],
  "angulardoc.lastSync": 0
}