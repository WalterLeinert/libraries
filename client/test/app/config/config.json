{
  "logging": {
    "appenders": [
      {
        "___comment": "z.Zt. nicht verwendet, es wird immer mit console gelogged",
        "type": "console",
        "levels": [
          "DEBUG"
        ],
        "layout": {
          "type": "pattern",
          "pattern": "%d{ISO8601} [%[%-5p%]] %[%-20c%] %m"
        }
      }
    ],
    "levels": {
      "[all]": "WARN",
      "Store": "WARN",
      "CommandStore": "WARN",
      "CommandStoreStorage": "INFO",
      "PassportService": "INFO",
      "Service": "DEBUG",
      "CoreComponent": "WARN",
      "ExtendedCoreComponent": "INFO",
      "DataTableSelectorComponent": "WARN",
      "SelectorBaseComponent": "WARN",
      "TimeSelectorComponent": "INFO",
      "AutoformComponent": "WARN",
      "LoginComponent": "INFO",
      "LogoffComponent": "INFO",

      "EntityGenerator": "INFO"
    }
  }
}
