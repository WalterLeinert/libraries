{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "client": [
        "dist/client"
      ],
      "client/*": [
        "dist/client/*"
      ],
      "@fluxgate/client": [
        "dist/client"
      ],
      "@fluxgate/client/*": [
        "dist/client/*"
      ],
      "@fluxgate/common": [
        "dist/common"
      ],
      "@fluxgate/common/*": [
        "dist/common/*"
      ],
      "@fluxgate/components": [
        "dist/components"
      ],
      "@fluxgate/components/*": [
        "dist/components/*"
      ],
      "@fluxgate/core": [
        "dist/core"
      ],
      "@fluxgate/node/*": [
        "dist/node/*"
      ],
      "@fluxgate/node": [
        "dist/node"
      ],
      "@fluxgate/core/*": [
        "dist/core/*"
      ],
      "@fluxgate/platform": [
        "dist/platform"
      ],
      "@fluxgate/platform/*": [
        "dist/platform/*"
      ],
      "@fluxgate/testing": [
        "dist/testing"
      ],
      "@fluxgate/testing/*": [
        "dist/testing/*"
      ]
    }
  }
}