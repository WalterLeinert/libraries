{
  "name": "@fluxgate/libraries",
  "version": "2.6.0-beta.0",
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist --concurrency 1",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "lerna clean --yes && rimraf dist/ *-error.log",
    "reallyclean": "npm run clean && rimraf node_modules/",
    "build:core": "lerna --scope @fluxgate/core exec gulp test",
    "build:platform": "lerna --scope @fluxgate/platform exec gulp test",
    "build:common": "lerna --scope @fluxgate/common exec gulp test",
    "build:node": "lerna --scope @fluxgate/node exec gulp test",
    "build:testing": "lerna --scope @fluxgate/testing exec gulp test",
    "build:server": "lerna --scope @fluxgate/server exec gulp test",
    "build:client": "ng build client",
    "build:components": "ng build components",
    "build:libraries": "npm run build:core && npm run build:platform && npm run build:common && npm run build:node && npm run build:testing && npm run build:server && npm run build:client && npm run build:components",
    "build:starter:common": "lerna --scope @fluxgate/starter-common exec gulp test",
    "build:starter:server": "lerna --scope @fluxgate/starter-server exec gulp test",
    "build:starter": "npm run build:starter:common && npm run build:starter:server && ng build starter",
    "build:all": "npm run build:libraries && npm run build:starter",
    "rebuild:all": "npm run reallyclean && npm run bootstrap && npm run build:all"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/WalterLeinert/libraries.git"
  },
  "author": "Walter Leinert <walter.leinert@outlook.com>",
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "^7.2.14",
    "@angular/common": "^7.2.14",
