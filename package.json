    "@angular/http": "^7.2.14",
    "@angular/platform-browser": "^7.2.14",
    "@angular/platform-browser-dynamic": "^7.2.14",
    "@angular/router": "^7.2.14",
    "angular-resizable-element": "^3.2.0",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "crypto-js": "^3.1.9-1",
    "http-status-codes": "^1.3.0",
    "ng-sidebar": "^8.0.0",
    "ngx-toastr": "^9.1.2",
    "primeicons": "1.0.0-beta.10",
    "primeng": "7.0.5",
    "rxjs": "~6.2.2",
    "typescript": "~3.2.4",
    "zone.js": "^0.8.26"
  },
