{
    "___comment": "Konfiguration f√ºr Unittests",
    "appenders": [
        {
            "type": "file",
            "___filename_windows": "griso.log",
            "___filename_docker": "/var/log/griso.log",
            "filename": "griso.log",
            "levels": [
                "DEBUG"
            ],
            "layout": {
                "type": "pattern",
                "pattern": "%d{ISO8601} [%-5p] %-20c %m"
            }
        },
        {
            "type": "console",
            "levels": [
                "DEBUG"
            ],
            "layout": {
                "type": "pattern",
                "pattern": "%d{ISO8601} [%[%-5p%]] %[%-20c%] %m"
            }
        }
    ],
    "levels": {
        "[all]": "ERROR",
        "Test": "TRACE",
        "Test2": "INFO",
        "ReduxTest": "INFO"
    }
}